{
  "name": "postnummer",
  "version": "1.0.1",
  "description": "Norske postnummer",
  "main": "src/index.js",
  "scripts": {
    "test": "tap --reporter=spec test/*.test.js",
    "lint": "eslint src",
    "lint:fix": "eslint --fix src",
    "coverage": "tap test/*.js --coverage",
    "refresh": "rm -rf node_modules && rm package-lock.json && npm install",
    "size-build": "npm install",
    "size": "size-limit",
    "size-watch": "size-limit --why"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/storbukas/postnummer.git"
  },
  "engines": {
    "node": ">=4"
  },
  "keywords": [
    "norsk",
    "postnummer",
    "posten",
    "bring",
    "Norge",
    "Norway",
    "postal codes"
  ],
  "author": "Lars Erik Storbuk√•s",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/storbukas/postnummer/issues"
  },
  "homepage": "https://github.com/storbukas/postnummer#readme",
  "devDependencies": {
    "@size-limit/preset-small-lib": "^4.4.5",
    "eslint": "^5.16.0",
    "eslint-config-airbnb-base": "^13.2.0",
    "eslint-plugin-import": "^2.18.2",
    "size-limit": "^4.4.5",
    "tap": "^14.10.7"
  },
  "size-limit": [
    {
      "path": "src/index.js"
    }
  ]
}
